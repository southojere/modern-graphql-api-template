{
  "name": "graphql-auth",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "seed": "ts-node src/seed/index.ts",
    "test": "echo run tests here",
    "migrate:up": "prisma migrate --experimental up --auto-approve --create-db",
    "studio": "npx prisma studio --experimental"
  },
  "dependencies": {
    "@nexus/schema": "^0.18.0",
    "@prisma/client": "^2.11.0",
    "@prisma/cli": "2.11.0",
    "bcryptjs": "2.4.3",
    "graphql": "^15.4.0",
    "graphql-middleware": "4.0.1",
    "graphql-shield": "7.4.1",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1",
    "nexus-plugin-prisma": "^0.24.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "12.12.59",
    "@types/ws": "7.2.6",
    "serverless": "^2.12.0",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-loader": "^8.0.11",
    "ts-node": "9.0.0",
    "ts-node-dev": "1.0.0-pre.50",
    "tslint": "^6.1.3",
    "typescript": "4.1.2"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
